@Getter
@NoArgsConstructor
@Entity
@Table(name = "chklist_item_map")
public class ChklistItemMap {
    //필드
    @Id
    @GeneratedValue(strategy = GenerationType.AUTO)
    @Column(name = "id", unique = true, nullable = false)
    private Long id;
    
    @Column(nullable = false)
    private Long chklist_id;
    
    @Column(nullable = false)
    private Long chklist_id;
}